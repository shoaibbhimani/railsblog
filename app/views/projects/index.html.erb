<h1 id="page_title">
	Stuff I have Built
</h1>

<div id="post_wrapper" class="sninny_wrapper">
	<% @projects.each do |project| %>
	  <div class="post">
		<div class="date">
			<%= project.created_at.strftime('%A, %B, %d') %> 
		</div>
		<h2> 
			<a href="#"><%= link_to project.title,  project_path(project) %></a> 
		</h2>
		<hr>
	</div>
	<% end %>

</div>
	

		
<%= will_paginate @projects %>
