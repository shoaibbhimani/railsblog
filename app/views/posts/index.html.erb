<h1>Hello World</h1>

<section>
	<% @posts.each do |post| %>
		<div>
			<h2>
				<%= post.title %>
			</h2>
			<%= post.content %>
			<div> <%= link_to 'Read Post', post_path(post) %></div>
			<div> <%= link_to 'Edit Post', edit_post_path(post) %></div>
			<div> <%= link_to 'Delete Post', post_path(post), method: :delete, data:{ confirm: "Are you sure ?" } %></div>
		</div>
	<% end %>
</section>
